# to be used only by DGL at BYU to generate the regression test standard

# first generate test results
cd testing
regression_test.bsh
cd ..

# archive results
tar cvf regres_test_RSS_results.tar testing/regress_RSS/*
gzip regres_test_RSS_results.tar

# move to output ftp site location
mv regres_test_RSS_results.tar.gz /brix/internet/ftp/outgoing/data/dl/mb/
