# compile script for meas_meta_sir.c
#
# written by DGL at BYU  4 Mar 2014


# requires netcdf library
gcc -O3 -m64 -I/auto/users/long/src/linux/netcdf/netcdf-4.0.1/include -I../sirlib/c/include meas_meta_sir1.c netcdf_dump.c ../sirlib/c/lib/sir_geom.c -L/auto/users/long/src/linux/netcdf/netcdf-4.0.1/lib -lnetcdf -L/auto/users/long/src/linux/netcdf/hdf5-1.8.3/lib -lhdf5_hl -lhdf5 -L/auto/users/long/src/linux/netcdf/zlib-1.2.3/lib -lz -lm -o meas_meta_sir1 
